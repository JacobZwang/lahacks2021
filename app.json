{
    "name": "LaHacks 2021",
    "env": {
        "NAME": {
            "description": "Name"
        },
        "ZONE": {
            "description": "Where do you want to host?",
            "value": "us-west2-a",
            "required": false
        },
        "LOGS_DB": {
            "description": "BigTable Logs DB ID - Must be atleast 6 characters long"
        }
    },
    "hooks": {
        "prebuild": {
            "commands": [
              "cbt createinstance $LOGS_DB $LOGS_DB $LOGS_DB $ZONE 1 SSD -project $GOOGLE_CLOUD_PROJECT"
            ]
        }
    }
}